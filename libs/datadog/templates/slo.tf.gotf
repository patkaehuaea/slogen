resource "datadog_service_level_objective" "{{ .TFResourceName }}" {
  name        = "{{.Name}}"
  type        = "metric"
  description = "{{.Description}}"
  query {
    numerator   = "{{.Query.Numerator}}"
    denominator = "{{.Query.Denominator}}"
  }

  thresholds {
    timeframe = "{{.Thresholds.Timeframe}}"
    target    = "{{.Thresholds.Target}}"
  }

  tags = [{{.Tags}}]
}